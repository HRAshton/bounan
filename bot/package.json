        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.525.0",
        "@aws-sdk/lib-dynamodb": "^3.525.0",
        "async": "^3.2.5",
        "async-mutex": "^0.4.1",
        "aws-sdk": "^2.1569.0",
        "axios": "^1.6.7",
